                android:background="#d2b48c"
                android:text=" " />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="0.1dp"
                android:background="#d2b48c"
                android:gravity="center"
                android:text="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="0.1dp"
