            android:gravity="center"
            android:layout_weight="1"
            android:text="天" />

        </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/ll1"
                android:orientation="horizontal"
                android:layout_marginTop="0.1dp"
                >

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/ll2"
                android:orientation="horizontal"
                android:layout_marginTop="0.1dp"
                >

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/ll3"
                android:orientation="horizontal"
                android:layout_marginTop="0.1dp"
               >

            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll4"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginTop="0.1dp"
                >

            </LinearLayout>
            <LinearLayout
